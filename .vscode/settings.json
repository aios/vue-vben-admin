{
  "typescript.tsdk": "./node_modules/typescript/lib",
  //===========================================
  //============= Editor ======================
  //===========================================
  "explorer.openEditors.visible": 0,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 300,
  "editor.minimap.showSlider": "always",
  "editor.smoothScrolling": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.detectIndentation": false, // vscode__Some-New-Token__tabsize__Some-New-Token__
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.formatOnPaste": true, //__Some-New-Token__
  "editor.formatOnSave": true, //__Some-New-Token__
  "editor.suggestSelection": "first",
  "editor.trimAutoWhitespace": true,
  "editor.quickSuggestions": {
    //   __Some-New-Token__
    "other": true,
    "comments": true,
    "strings": true
  },
  //===========================================
  //============= Other =======================
  //===========================================
  "breadcrumbs.enabled": true, // __Some-New-Token__/__Some-New-Token__
  "open-in-browser.default": "chrome", // __Some-New-Token__
  //===========================================
  //============= emmet =======================
  //===========================================
  "emmet.triggerExpansionOnTab": true, // __Some-New-Token__emmet__Some-New-Token__tab__Some-New-Token__
  "emmet.showAbbreviationSuggestions": true,
  "emmet.showExpandedAbbreviation": "always",
  "emmet.syntaxProfiles": {
    // __Some-New-Token__emmet__Some-New-Token__，__Some-New-Token__vue__Some-New-Token__html__Some-New-Token__emmet__Some-New-Token__
    "vue-html": "html",
    "vue": "html",
    "javascript": "javascriptreact",
    // xml__Some-New-Token__，__Some-New-Token__emmet__Some-New-Token__
    "xml": {
      "attr_quotes": "single"
    }
  },
  "emmet.includeLanguages": {
    // __Some-New-Token__react__Some-New-Token__jsx__Some-New-Token__emmet__Some-New-Token__
    "jsx-sublime-babel-tags": "javascriptreact"
  },
  //===========================================
  //============= files =======================
  //===========================================
  // "files.autoSave": "onWindowChange", // __Some-New-Token__
  // "files.autoSaveDelay": 1000, // __Some-New-Token__
  "files.trimTrailingWhitespace": true, // __Some-New-Token__，__Some-New-Token__。
  // __Some-New-Token__
  "files.insertFinalNewline": true,
  // __Some-New-Token__
  "files.trimFinalNewlines": true,
  "files.eol": "\n",
  "search.exclude": {
    // __Some-New-Token__
    "**/node_modules": true,
    "**/*.log": true,
    "**/*.log*": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/elehukouben": true,
    "**/.git": true,
    "**/.gitignore": true,
    "**/.svn": true,
    "**/.DS_Store": true,
    "**/.idea": true,
    "**/.vscode": false,
    "**/yarn.lock": true,
    "**/tmp": true
  },
  "files.exclude": {
    // __Some-New-Token__，__Some-New-Token__node_modules(__Some-New-Token__)
    // "**/node_modules": true,
    "**/bower_components": true,
    "**/.idea": true,
    "**/yarn.lock": true,
    "**/tmp": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "files.watcherExclude": {
    // __Some-New-Token__
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.vscode/**": true,
    "**/node_modules/**": true,
    "**/tmp/**": true,
    "**/bower_components/**": true,
    "**/dist/**": true,
    "**/yarn.lock": true
  },
  "files.associations": {
    // __Some-New-Token__，__Some-New-Token__，__Some-New-Token__wxss__Some-New-Token__css
    "*.vue": "vue",
    "*.wxss": "css"
  },
  "stylelint.enable": true,
  "stylelint.packageManager": "yarn",
  "css.validate": true,
  "less.validate": true,
  "scss.validate": true,
  // ↓↓↓↓↓↓↓↓↓↓↓↓↓ __Some-New-Token__ ↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓
  // ↓↓↓↓↓↓↓↓↓↓↓↓↓ __Some-New-Token__ ↓↓↓↓↓↓↓↓↓↓↓↓
  // ↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓
  // ===========================================
  // ================ Eslint ===================
  // ===========================================
  "eslint.enable": true, // __Some-New-Token__eslint
  "eslint.options": {
    // __Some-New-Token__
    "plugins": [
      "html",
      "vue",
      "javascript",
      "jsx",
      "typescript"
    ]
  },
  "eslint.autoFixOnSave": true, // __Some-New-Token__
  // ===========================================
  // ================ Vetur ====================
  // ===========================================
  "vetur.experimental.templateInterpolationService": true,
  "vetur.format.options.tabSize": 2,
  "vetur.format.defaultFormatter.html": "js-beautify-html", // __Some-New-Token__js-beautify-html__Some-New-Token__
  "vetur.format.defaultFormatter.scss": "prettier", // __Some-New-Token__js-beautify-html__Some-New-Token__
  "vetur.format.defaultFormatter.css": "prettier", // __Some-New-Token__js-beautify-html__Some-New-Token__
  //   "vetur.format.defaultFormatter.html": "prettyhtml",
  "vetur.format.defaultFormatter.ts": "prettier-tslint",
  "vetur.format.defaultFormatter.js": "prettier",
    "vetur.languageFeatures.codeActions": false,
  // "vetur.useWorkspaceDependencies": true,
  "vetur.format.defaultFormatterOptions": {
    "js-beautify-html": {
      //   "wrap_attributes": "force-aligned", // __Some-New-Token__
      "wrap_attributes": "force-expand-multiline" // __Some-New-Token__ __Some-New-Token__
    },
    "prettier": {
      "eslintIntegration": true, // __Some-New-Token__perttier__Some-New-Token__eslint__Some-New-Token__
      "arrowParens": "always", // __Some-New-Token__ __Some-New-Token__avoid __Some-New-Token__ avoid | always
      "semi": false, // __Some-New-Token__, __Some-New-Token__true
      "singleQuote": true // __Some-New-Token__, __Some-New-Token__false(__Some-New-Token__jsx__Some-New-Token__, __Some-New-Token__)
    }
  },
  // __Some-New-Token__
  //===========================================
  //============= Code Runner =================
  //===========================================
  "javascript.updateImportsOnFileMove.enabled": "never",
  "liveServer.settings.donotShowInfoMsg": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.rendererType": "dom", //__Some-New-Token__liveserver__Some-New-Token__
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  // __Some-New-Token__
  "path-intellisense.mappings": {
    "/@/": "${workspaceRoot}/src"
  },
  "prettier.requireConfig": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.sideBar.location": "left",

  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[less]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
